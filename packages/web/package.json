{
    "name": "@paczkoapi/web",
    "version": "0.6.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/paczkoapi/paczkoapi.git"
    },
    "license": "MIT",
    "exports": {
        ".": {
            "import": "./dist/lib/lib.esm.js",
            "require": "./dist/lib/lib.cjs.js"
        },
        "./components/*": {
            "import": "./dist/components/*.js",
            "types": "./dist/components/*.d.ts",
            "default": "./dist/components/*.js"
        },
        "./loader": {
            "import": "./dist/loader/index.js",
            "require": "./dist/loader/index.cjs.js",
            "types": "./dist/loader/index.d.ts",
            "default": "./dist/loader/index.js"
        }
    },
    "main": "dist/index.cjs.js",
    "unpkg": "dist/lib/lib.esm.js",
    "module": "dist/lib/lib.esm.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "stencil build",
        "build-storybook": "storybook build",
        "dev": "storybook dev -p 6006 --no-open",
        "generate": "stencil generate",
        "serve": "stencil build --watch --dev --serve --config stencil.config.serve.ts",
        "storybook": "storybook dev -p 6006",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "watch": "stencil build --watch --config stencil.config.dev.ts"
    },
    "dependencies": {
        "@nzyme/apis": "^0.12.1",
        "@nzyme/dom-utils": "^0.12.1",
        "@nzyme/utils": "^0.12.1",
        "@paczkoapi/client": "0.6.0",
        "@stencil/core": "^4.29.2",
        "lodash.debounce": "^4.0.8"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.6",
        "@nzyme/eslint": "^0.12.1",
        "@nzyme/project-utils": "^0.12.1",
        "@nzyme/tsconfig": "^0.12.1",
        "@paczkoapi/common": "0.6.0",
        "@stencil/sass": "^3.2.1",
        "@storybook/addon-essentials": "^8.6.12",
        "@storybook/addon-events": "^6.2.9",
        "@storybook/blocks": "^8.6.12",
        "@storybook/test": "^8.6.12",
        "@storybook/web-components": "^8.6.12",
        "@storybook/web-components-vite": "^8.6.12",
        "@types/jest": "^29.5.14",
        "@types/lodash.debounce": "^4.0.9",
        "@types/node": "^22.14.0",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "lit": "^3.2.1",
        "puppeteer": "^24.6.0",
        "storybook": "^8.6.12",
        "vite": "^6.2.5"
    },
    "publishConfig": {
        "access": "public"
    },
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js"
}
