        "smoke:mjs": "node tests/smoke.test.mjs"
    },
    "dependencies": {
        "@nzyme/types": "^0.9.0",
        "@nzyme/utils": "^0.9.0",
        "@paczkoapi/common": "0.5.0",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@nzyme/eslint": "^0.9.0",
        "@nzyme/tsconfig": "^0.9.0",
        "@nzyme/vitest": "^0.9.0",
        "typescript": "^5.8.2"
    },
    "publishConfig": {
        "access": "public"
    }
