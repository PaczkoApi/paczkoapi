{
    "extends": "@nzyme/tsconfig/esm.json",
    "compilerOptions": {
        "rootDir": "./src",
        "outDir": "./dist",
        "tsBuildInfoFile": "./tsconfig.esm.tsbuildinfo",
        "emitDeclarationOnly": true,
        "moduleResolution": "node",
        "module": "ESNext",
        "jsx": "preserve",
        "jsxImportSource": "@builder.io/mitosis",
        "lib": ["ESNext", "DOM", "DOM.Iterable"]
    },
    "include": ["src"]
}
