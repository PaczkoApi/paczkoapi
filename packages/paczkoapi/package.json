{
    "name": "paczkoapi",
    "version": "0.6.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/paczkoapi/paczkoapi.git"
    },
    "license": "MIT",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.cjs"
            },
            "default": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            }
        },
        "./lazy": {
            "import": {
                "types": "./dist/index.lazy.d.ts",
                "default": "./dist/index.lazy.mjs"
            },
            "require": {
                "types": "./dist/index.lazy.d.ts",
                "default": "./dist/index.lazy.cjs"
            },
            "default": {
                "types": "./dist/index.lazy.d.ts",
                "default": "./dist/index.lazy.mjs"
            }
        },
        "./package.json": "./package.json",
        "./types": "./src/shims.d.ts"
    },
    "typesVersions": {
        "*": {
            ".": [
                "./dist/index.d.ts"
            ],
            "./lazy": [
                "./dist/index.lazy.d.ts"
            ]
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "ENV=prod rollup -c",
        "eslint": "eslint . --fix --cache",
        "smoke:cjs": "node tests/smoke.test.cjs",
        "smoke:mjs": "node tests/smoke.test.mjs",
        "watch": "rollup -c --watch"
    },
    "dependencies": {
        "@paczkoapi/common": "^0.6.0",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@nzyme/eslint": "^0.12.1",
        "@nzyme/esm": "^0.12.1",
        "@nzyme/tsconfig": "^0.12.1",
        "@nzyme/vitest": "^0.12.1",
        "@paczkoapi/web": "^0.6.0",
        "@rollup/plugin-commonjs": "^28.0.3",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "rollup": "^4.39.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "typedoc": "^0.28.1",
        "typedoc-plugin-markdown": "^4.6.1",
        "typedoc-vitepress-theme": "^1.1.2",
        "typescript": "^5.8.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
